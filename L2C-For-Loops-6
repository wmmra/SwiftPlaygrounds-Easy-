func backToCtr() {
    turnLeft()
    turnLeft()
    moveForward()
    moveForward()
}

func twoSwitches() {
    for i in 1 ... 2 {
        moveForward()
        toggleSwitch()
    }
    
}

func twoGems() {
    for i in 1 ... 2 {
        moveForward()
        collectGem()        
    }
}

for i in 1 ... 3 {
    turnLeft()
    twoSwitches()
    backToCtr()
    twoGems()
    backToCtr()
    turnRight()
    moveForward()
}
